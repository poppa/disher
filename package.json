    "bootstrap": "yarn install --pure-lockfile",
    "build": "lerna run build",
    "dist": "yarn clean && yarn gql && yarn build",
    "test": "lerna run test",
    "lint": "yarn eslint packages/**/*.ts --quiet",
    "clean": "lerna run clean",
    "clean:all": "lerna run clean:all && rimraf node_modules",
